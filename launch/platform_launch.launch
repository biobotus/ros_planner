  <node pkg="rosserial_python" type="serial_node.py" name="rosserial" output="screen" machine="pi1" required="True"/>

  <machine name="pi2" address="pi2" env-loader="/home/ubuntu/env.sh" user="ubuntu" password="ubuntu" />
